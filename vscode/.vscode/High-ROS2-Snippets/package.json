{
	"name": "high-ros2-snippets",
	"displayName": "High ROS2 Snippets",
	"description": "ROS2 (Robot Operating System 2) snippets for rclpy, rclcpp, launch(xml), urdf, xacro etc.",
	"version": "0.0.1",
     "publisher": "SeaHI-Robot",
	"icon": "media/icon.png",
	"keywords": [
		"ros",
		"robot"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SeaHI-Robot/High-ROS2-Snippets"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "xml",
				"extensions": [
					"launch",
					"srdf",
					"urdf",
					"xacro"
				]
			},
			{
				"id": "yaml",
				"extensions": [
					"rviz"
				]
			}
		],
		"snippets": [
			{
				"language": "xml",
				"path": "./snippets/ros_urdf_snippets.json"
			},
			{
				"language": "xml",
				"path": "./snippets/ros_launch_file.json"
			},
			{
				"language": "python",
				"path": "./snippets/ros_py_snippets.json"
			},
			{
				"language": "python",
				"path": "./snippets/ros_py_moveit.json"
			},
			{
				"language": "cpp",
				"path": "./snippets/ros_cpp_snippets.json"
			}
		]
	}
}
